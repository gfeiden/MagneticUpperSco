@article{Doyle1990a,
	__Markedentry = {[gfeiden:]},
	Abstract = {{Five flares were observed on the late-type binary YY Gem in March
	1988 during a total monitoring time of 408 min. The flares were unusual
	in that there is a periodicity in the occurrence of four of them,
	these being separated by 48 + or - 3 min. Considering the flares
	to be formed as a stochastic process, it is found that the probability
	of these events occurring by chance is 0.5 percent. Modeling indicates
	that for quite reasonable input parameters, the flare periodicity
	can be explained in terms of filament oscillations. The only requirement
	is that there should be a filament at these heights where the magnetic
	field drops inversely proportional to the height. }},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/1990A%26A...232...83D},
	Author = {{Doyle}, J.~G. and {Butler}, C.~J. and {van den Oord}, G.~H.~J. and {Kiang}, T.},
	Journal = {\aap},
	Keywords = {ECLIPSING BINARY STARS, LATE STARS, STELLAR FLARES, PERIODIC VARIATIONS, STOCHASTIC PROCESSES},
	Month = jun,
	Owner = {gfeiden},
	Pages = {83-88},
	Timestamp = {2013.02.21},
	Title = {{A periodicity in the flaring rate on the eclipsing binary YY Geminorum}},
	Volume = {232},
	Year = {1990}}

@article{Doyle1990b,
	__Markedentry = {[gfeiden:]},
	Abstract = {{The observed flare activity on the eclipsing binary star YY Geminorum,
	as determined by the time-average flare energy, is more than as order
	of magnitude greater out-of-eclipse than that during eclipses. The
	out-of-eclipse value is however, heavily biased by four large flares.
	In the cumulative distribution diagram these larger flares have a
	substantially different statistical occurrence rate than the remainder
	of the flares. It is suggested that the small separation between
	the two stars leads to significantly enhanced magnetic field between
	the two components and thus to the occurrence of energetic flares,
	observable when the stars are out of eclipse. }},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/1990A%26A...227..130D},
