	Archiveprefix = {arXiv},
	Author = {{Torres}, G.},
	Doi = {10.1086/524886},
	Eprint = {0710.4883},
	Journal = {\apjl},
	Keywords = {Stars: Planetary Systems, Stars: Evolution, Stars: Fundamental Parameters, Stars: Individual: Alphanumeric: GJ 436, Stars: Low-Mass, Brown Dwarfs},
	Month = dec,
	Owner = {gfeiden},
	Pages = {L65-L68},
	Timestamp = {2013.02.14},
	Title = {{The Transiting Exoplanet Host Star GJ 436: A Test of Stellar Evolution Models in the Lower Main Sequence, and Revised Planetary Parameters}},
	Volume = {671},
