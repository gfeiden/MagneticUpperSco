	Doi = {10.1086/145994},
	Journal = {\apj},
	Month = mar,
	Owner = {gfeiden},
	Pages = {349},
	Timestamp = {2013.02.14},
	Title = {{The Sun's Magnetic Field, 1952-1954.}},
	Volume = {121},
	Year = {1955},
	Bdsk-Url-1 = {http://dx.doi.org/10.1086/145994}}

