	Number = {1},
	Pages = {97--111},
	Title = {{On the apsidal-motion of thirteen eclipsing binaries}},
	Url = {http://arxiv.org/abs/1204.5578},
	Volume = {62},
	Year = {2012},
	Bdsk-Url-1 = {http://arxiv.org/abs/1204.5578}}

