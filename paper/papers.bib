	Eprint = {arXiv:astro-ph/0512072},
	Journal = {\apj},
	Keywords = {Stars: Binaries: Close, Stars: Binaries: Spectroscopic, Stars: Evolution, stars: individual (V1061 Cyg), Techniques: Spectroscopic},
